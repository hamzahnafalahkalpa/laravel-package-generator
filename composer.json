{
    "name": "hanafalah/laravel-package-generator",
    "description": "Package yang membantu anda dalam membuat sebuah package",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Hanafalah\\LaravelPackageGenerator\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "authors": [
        {
            "name": "hamzah nur al falah",
            "email": "hamzahnafalah@gmail.com"
        }
    ],
    "require": {
        "hanafalah/laravel-support": "dev-main",
        "hanafalah/laravel-stub": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hanafalah\\LaravelPackageGenerator\\LaravelPackageGeneratorServiceProvider"
            ]
        }
    }
}
